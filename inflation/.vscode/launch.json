      "cwd": "${workspaceRoot}",
      "args": [
        "--lik_weights",
        "data/lik_weights.xlsx",
        "--lik_evolution",
        "data/lik_evolution.xlsx",
        "--credentials_file",
        "sql_credentials.json",
        "--create_sql_table"
